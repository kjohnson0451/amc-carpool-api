{
  "presets": ["@babel/preset-env"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@config": "./src/config",
          "@controllers": "./src/controllers",
          "@middleware": "./src/middleware",
          "@models": "./src/models",
          "@routes": "./src/routes",
          "@services": "./src/services",
          "@utils": "./src/utils"
        }
      }
    ],
    "@babel/plugin-transform-modules-commonjs"
  ]
}
